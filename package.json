    "cypress": "^7.7.0",
    "cypress-axe": "^0.12.0",
    "cypress-file-upload": "^5.0.2",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "cypress-xpath": "^1.6.1",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
